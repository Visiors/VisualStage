<?xml version="1.0" encoding="UTF-8"?>
<definitions>
		
		<symbol id="edge_selection_marker"  >
				<rect x="0" y="0" width="5" height="5" style="fill:darkred;stroke:none"/>
				<rect x="0" y="0" width="4" height="4" style="fill:tomato;stroke:none"/>
		</symbol>

		<symbol id="edge_manipulation_marker" width="9" height="9">
				<polygon points="0,3 3,0 6,3 3,6" style="fill:green;stroke:darkgreen"/>
		</symbol>

		<symbol id="node_selection_marker" width="5" height="5">
				<rect x="0" y="0" width="5" height="5" style="fill:darkred;stroke:none"/>
				<rect x="0" y="0" width="4" height="4" style="fill:tomato;stroke:none"/>
		</symbol>

		<symbol id="subgraph_selection_marker"  width="5" height="5">
				<rect x="0" y="0" width="5" height="5" style="fill:dimgray;stroke:none"/>
				<rect x="0" y="0" width="4" height="4" style="fill:darkgray;stroke:none"/>
		</symbol>

		<symbol id="node_port_indicator" width = "11" height="11">
				<circle cx="5" cy="5" r="5" stroke="gray" stroke-width="0.5" fill="white"/>
		</symbol>

		<symbol id="node_port_highlight_indicator" width = "11" height="11">
				<circle cx="5" cy="5" r="5" stroke="darkgray" stroke-width="0.5" fill="red"/>
		</symbol>
			
		<polyline  id="edge_baseline" points="" 
			style="fill:none;stroke:darkgray;stroke-width:1; stroke-dasharray:1 3" />

</definitions>